{
    "name": "blossom-e2e-tests",
    "version": "1.0.0",
    "description": "e2e tests for blossom authorization and asset chaincode",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "start": "npm run build && node dist/app.js",
        "build": "../../../e2e/tests/node_modules/.bin/tsc",
        "build:watch": "../../../e2e/tests/node_modules/.bin/tsc -w",
        "prepublishOnly": "npm run build",
        "test": "jest"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "esrun": "^3.2.26",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "ts-node": "10.9.1",
        "ts-node-dev": "2.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.10",
        "ts-jest": "^29.1.1",
        "tslint": "^5.11.0",
        "typescript": "4.3.5"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
